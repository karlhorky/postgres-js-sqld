{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@upleveled/eslint-config-upleveled/tsconfig.base.json",
  "compilerOptions": {
    "module": "node16",
    "target": "es2022"
  },
  "include": [
    "**/.eslintrc.cjs",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "**/*.cjs",
    "**/*.mjs",
    ".next/types/**/*.ts",
    "next-env.d.ts"
  ]
}
